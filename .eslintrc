{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "eol-last": "off",
//    "quotes": [2, "single", { "avoidEscape": true }],
    "operator-linebreak": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}